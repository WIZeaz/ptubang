<!--components/w-swiper/w-swiper.wxml-->
<swiper circular="true" autoplay interval="1500" indicator-dots style="height:{{Hei}}px" indicator-active-color="#ff5777" class="swiper">
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image src="{{item}}"mode="widthFix" bindload='imgH' class="swiper-item-img" ></image>
    </swiper-item>
  </block>
</swiper> 
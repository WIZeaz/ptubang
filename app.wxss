/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.navbar{
  position: absolute;
  bottom: 0px;
  width: 100%;
  min-height: 3rem;
  font-size: 10px;
  border-top: #333 1px solid;
}
.nav-item{
  display: inline-block !important; 
  width: 33%;
  height: 100%;
  text-align: center;
  line-height: 3rem;
}
.block{
  display: block;
}

.text-center{
  margin: 0 auto;
  text-align: center;
}
